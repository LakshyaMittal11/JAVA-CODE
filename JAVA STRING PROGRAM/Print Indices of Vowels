import java.util.Scanner;
public class P4 {
public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
String str1=sc.next();
print(str1);
}
public static void print(String str){
for(int i=0;i<str.length();i++){
char ch =str.charAt(i);
if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'){
System.out.print(i+" ");
}
}
}
}
