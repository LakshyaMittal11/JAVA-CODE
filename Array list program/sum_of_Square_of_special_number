import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sumOfSquares = 0;
        for (int i = 1; i <= n; i++) {
            int num = sc.nextInt();
            if (n % i == 0) { 
                sumOfSquares += num * num; 
            }
        }
        System.out.println(sumOfSquares);
    }
}
